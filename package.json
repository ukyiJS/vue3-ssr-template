{
  "name": "vue3-ssr-template",
  "version": "1.0.0",
  "description": "vue3-ssr-template",
  "license": "MIT",
  "author": "ukyi",
  "engines": {
    "node": ">=16.14.0"
  },
  "volta": {
    "node": "16.14.0"
  },
  "scripts": {
    "dev": "cross-env VITE=1 esno server/dev-server",
    "preview": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=1024\" esno dist/server --host 0.0.0.0",
    "build": "rm -rf dist && run-s build:app build:server",
    "build:app": "rm -rf dist && run-s build:app-client build:app-server",
    "build:app-client": "cross-env NODE_ENV=production webpack --config build/webpack.app-client",
    "build:app-server": "cross-env NODE_ENV=production webpack --config build/webpack.app-server",
    "build:server": "rm -rf dist/server && cross-env NODE_ENV=production webpack --config build/webpack.server"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.6",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "helmet": "^5.0.2",
    "js-yaml": "^4.1.0",
    "pinia": "^2.0.13",
    "regenerator-runtime": "^0.13.9",
    "serve-static": "^1.15.0",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@swc/core": "^1.2.172",
    "@types/compression": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.29",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "chalk": "^5.0.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.7.1",
    "esno": "^0.14.1",
    "express": "^4.18.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "swc-loader": "^0.2.0",
    "ts-loader": "^9.2.9",
    "typescript": "^4.6.3",
    "vite": "^2.9.6",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  }
}
